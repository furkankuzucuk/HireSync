{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
      "jsx": "react",
      "moduleResolution": "node",
      "module": "esnext",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": true,
      "baseUrl": "src",
      "paths": {
        "*": ["node_modules/*", "src/*"]
      }
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  